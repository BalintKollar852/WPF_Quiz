<Page x:Class="WPF_Quiz.Quiz"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Quiz"
        mc:Ignorable="d"
        Title="Quiz" Height="450" Width="800">
    <Grid Background="#040c0e">
        <StackPanel>
            <Label x:Name="Previous_resultslabel" Content="eddigi eredményei: " Foreground="White"></Label>
            <TextBox x:Name="Previous_resultstext" Height="50" IsReadOnly="True"></TextBox>
            <Label Content="Témakör:" Foreground="White"></Label>
            <TextBox x:Name="Topic_text" IsReadOnly="True"></TextBox>
            <Label Content="Questions_label" Foreground="White"></Label>
            <TextBox x:Name="Questions_text" IsReadOnly="True"></TextBox>
        </StackPanel>
        <Frame x:Name="MainFrame" Margin="-83,0,-83,0"/>
    </Grid>
</Page>
